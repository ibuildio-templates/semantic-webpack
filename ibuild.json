{
  "prompts": {
    "name": {
      "type": "string",
      "required": true,
      "message": "Project name"
    },
    "description": {
      "type": "string",
      "required": false,
      "message": "Project description",
      "default": "A Vue.js project"
    },
    "author": {
      "type": "string",
      "message": "Author"
    },
    "private": {
      "type": "boolean",
      "default": true
    },
    "vue": {
      "type": "boolean",
      "default": true
    },
    "lint": {
      "type": "confirm",
      "message": "Use ESLint to lint your code?"
    }
  }
}
